{
  "plugin-module": {
    "prefix": "lpmod",
    "body": [
      "local avail, p = pcall(require, \"$1\")",
      "if not avail then",
      "  return",
      "end",
      "",
      "p.setup({$2})"
    ],
    "description": "Skeleton for a plugin config module"
  },
  "local-module": {
    "prefix": "lmod",
    "body": [
      "local M = {}",
      "",
      "function M.setup($1)",
      "\t$2",
      "end",
      "",
      "return M"
    ],
    "description": "Skeleton for a module"
  }
}
